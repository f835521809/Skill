
.box{
   display: flex;
   justify-content: space-between;
   background: #ccc;
   height: 150rpx;
 
}
.box1 text{
   display: block;
}
.set{
   width: 200rpx;
   height: 70rpx;
   border-radius: 50rpx;
   border: 2px solid #fff; 
   text-align: center;
   margin-top: 25rpx;
}
.time{
  width: 695rpx;
  margin-left: 30rpx;
  background: #fff;
  border-radius: 10rpx;
  margin-top: 30rpx;
}
.time ul{
  width: 100%;
  display: flex;
  margin-top: 30rpx;
  flex-wrap: wrap;
  padding-bottom: 30rpx;
}
.time ul li{
  width: 145rpx;
  height: 90rpx;
  background: #eee;
  color: #666;
  font-size: 28rpx;
  border-radius: 10rpx;
  border: 1px solid #eee;
  margin-left: 22rpx;
  margin-top: 20rpx;
}
.time ul li text{
  display: block;
  margin-top: 9rpx;
  margin-left: 12rpx;
}
.btn{
  width: 100%;
  height: 100rpx;
  margin-top: 60rpx;
}
.btns{
  width: 695rpx;
  height: 100rpx;
  margin-left: 30rpx;
  border-radius: 10rpx;
  color: #fff;
  font-size: 35rpx;
  text-align: center;
  line-height: 100rpx;
  background: #5E3862;
}
.active{
  border:1px solid #5E3862!important;
  color: #5E3862!important;
}
.enrolled{
  width: 695rpx;
  height: 200rpx;
  background: #fff;
  margin-left: 30rpx;
  border-top: 1px solid #eee;
}
.enrolled>text{
  display:block;
  width: 100%;
  font-size: 34rpx;
  padding: 20rpx 20rpx 0 20rpx;
}
.enrolled ul{
  width: 100%;
  display: flex;
  margin-top: 10rpx;
  flex-wrap: wrap;
  padding-bottom: 30rpx;
}
.enrolled ul li{
  width: 150rpx;
  height: 90rpx;
  background: #eee;
  color: #666;
  font-size: 28rpx;
  border-radius: 10rpx;
  border: 1px solid #eee;
  margin-left: 18rpx;
  margin-top: 20rpx;
  text-align: center;
  line-height: 90rpx;
}

button{
 margin-top: 250rpx;
}