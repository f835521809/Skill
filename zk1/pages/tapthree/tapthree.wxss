/* pages/taptwo/taptwo.wxss */

.box{
  width: 695rpx;
  margin-left: 30rpx;
  background: #fff;
  border-radius: 15rpx;
  margin-top: 30rpx;
  height: 360rpx;
}
.title{
  width: 100%;
  height: 100rpx;
  border-bottom: 1px dotted #ccc;
  line-height: 100rpx;
}
.title text:nth-child(1){
  float: left;
  margin-left: 30rpx;
}
.title text:nth-child(2){
  float: right;
  margin-right:30rpx;
  color: #666;
  font-size: 34rpx;
}
.center{
  width: 100%;
  height: 120rpx;
  border-bottom: 1px dotted #ccc;
}
.txt{
  width: 100%;
  height: 100rpx;
}
.bottom{
  width: 100%;
}
.txt text:nth-child(1){
  margin-top: 20rpx;
}
.txt text{
  display: block;
  font-size: 30rpx;
  color: #ccc;
  margin-left: 30rpx;
  line-height: 90rpx;
}
.bottom{
  display: flex;
}
.btn{
  width:130rpx;
  height:65rpx;
  text-align: center;
  line-height: 65rpx;
  border-radius: 20rpx;
  border:1px solid #5E3862;
  color: #5E3862;
  margin-top: 25rpx;
  margin-left: 30rpx;
}
.bottom>text{
  display: inline-block;
  font-size: 30rpx;
  color: #ccc;
  margin-left:30rpx;
  margin-top: 35rpx;
}
.center>text{
  display: block;
  font-size: 32rpx;
  color: #666;
  margin-left: 30rpx;
  margin-top: 15rpx;
}
.button{
  width: 100%;
  margin-top: 60rpx;
}
.button button{
  width: 695rpx;
  margin-left: 30rpx;
  margin-top: 10rpx;
  background: #F56C6C;
  color: #fff;
}